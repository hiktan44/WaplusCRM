{
  "version": 2,
  "builds": [
    {
      "src": "simple-whatsapp-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "simple-whatsapp-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  }
}